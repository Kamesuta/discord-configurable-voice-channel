generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model BlackLists {
  id            BigInt @id @default(autoincrement())
  user_id       String
  block_user_id String
}

model RoomLists {
  id              BigInt  @id @default(autoincrement())
  channel_id      String  @unique
  wait_channel_id String?
}
