{
  "name": "nodejs-typescript-eslint-template",
  "version": "1.0.0",
  "description": "Robust TypeScript Template with eslint for Node.js",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "ts-node src/index.ts",
    "clean": "rimraf dist",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc",
    "lint": "tsc --noEmit && eslint --cache --fix",
    "prettier": "prettier --write",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "pm2:start": "pm2 start dist/index.js --name Kokone --namespace DiscordBOT",
    "pm2:restart": "pm2 restart Kokone",
    "pm2:stop": "pm2 stop Kokone"
  },
  "author": "Kamesuta",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.5.8",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsdoc": "^46.5.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prisma": "^5.6.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "log4js": "^6.9.1",
    "node-cron": "^3.0.3",
    "pm2": "^5.3.0",
    "toml": "^3.0.0"
  }
}
